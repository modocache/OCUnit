{
    Class = Native;
    ProductType = "com.apple.product-type.framework";
    Description = "Target for building and unit testing a framework that uses Cocoa APIs.";
    CustomBuildSettings = {
        DYLIB_COMPATIBILITY_VERSION = "1";
        DYLIB_CURRENT_VERSION = "1";
        FRAMEWORK_VERSION = "A";
        INFOPLIST_FILE = "ÇPRODUCTNAMEÈ-Info.plist";
        INSTALL_PATH = "$(LOCAL_LIBRARY_DIR)/Frameworks";
        OTHER_CFLAGS = "-fobjc-exceptions";
        OTHER_LDFLAGS = "-framework Foundation -framework AppKit -framework SenTestingKit";
        GCC_PREFIX_HEADER = "$(SYSTEM_LIBRARY_DIR)/Frameworks/AppKit.framework/Headers/AppKit.h";
        GCC_PRECOMPILE_PREFIX_HEADER = YES;
        PRODUCT_NAME = "ÇPRODUCTNAMEÈ";
        GCC_WARN_FOUR_CHARACTER_CONSTANTS = NO;
        GCC_WARN_UNKNOWN_PRAGMAS = NO;
        WARNING_CFLAGS = "-Wmost";
	TEST_AFTER_BUILD = YES;
    };
    CustomProductSettings = {
        CFBundleExecutable = "ÇPRODUCTNAMEÈ";
        CFBundleInfoDictionaryVersion = "6.0";
        CFBundleVersion = "1.0.0d1";
        CFBundleIdentifier = "com.MySoftwareCompany.ÇTARGETNAMEASIDENTIFIERÈ";
        CFBundleDevelopmentRegion = English;
        CFBundlePackageType = "FMWK";
        CFBundleSignature = "????";
        CFBundleShortVersionString = "";
        CFBundleGetInfoString = "";
    };
	BuildPhases = (
        {
            Class = Headers;
        },
        {
            Class = Resources;
        },
        {
            Class = Sources;
        },
        {
            Class = Frameworks;
        },
        {
	    	Class = ShellScript;
		ShellPath = /bin/sh;
		ShellScript = "~/Developer/Tools/RunTargetUnitTests";
        },
	);
}
